{
    "config": {
        "secure-http": false
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.5",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/4.5.zip"
                },
                "require" : {
                    "fancyguy/webroot-installer": "1.1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        },
        {
            "type": "git",
            "url": "git@github.com:USStateDept/mailchimp-for-wp-pro.git"
        },
        {
            "type": "git",
            "url": "git@github.com:USStateDept/advanced-custom-fields.git"
        },
        {
            "type": "git",
            "url": "git@github.com:USStateDept/flexible-sidebar.git"
        },
        {
            "type": "git",
            "url": "git@github.com:IIP-Design/corona.git"
        },
        {
            "type": "git",
            "url": "git@github.com:USStateDept/ylai-theme.git"
        }
    ],
    "require": {
            "wordpress": "4.5",
            "fancyguy/webroot-installer": "1.1.0",
            "koodimonni/composer-dropin-installer": "0.2.4",
            "iip-design/corona": "dev-master",
            "wpackagist-plugin/addthis": "5.3.1",
            "wpackagist-plugin/wordpress-mu-domain-mapping": "0.5.5.1",
            "wpackagist-plugin/mailchimp-for-wp": "3.1",
            "wpackagist-plugin/google-apps-login": "2.9.6",
            "wpackagist-plugin/ewww-image-optimizer": "2.7.2",
            "wpackagist-plugin/wordpress-seo": "3.2.3",
            "wpackagist-plugin/amazon-ses-and-dkim-mailer": "1.7",
            "wpackagist-plugin/mailchimp-top-bar": "1.2.10",
            "usstatedept/flexible-sidebar": "1.0.0",
            "usstatedept/mailchimp-for-wp-pro": "3.1",
            "usstatedept/ylai": "dev-master"
        },
    "extra": {
        "dropin-paths": {
            "wp-content/": [
                "package:wpackagist-plugin/wordpress-mu-domain-mapping:sunrise.php"
            ]
        },
        "webroot-dir": "wp",
        "webroot-package": "wordpress"
    }
}
